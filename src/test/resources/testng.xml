<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Playwright Suite">

    <listeners>
        <listener class-name="com.qa.listeners.ExtentListener"/>
    </listeners>

    <test name="Sanity Tests">
        <parameter name="url" value="https://google.com"/>
        <classes>
            <class name="com.qa.tests.SanityTest"/>
        </classes>
    </test>

</suite>
